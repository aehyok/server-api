  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Lychee.Extensions\Lychee.Extensions.csproj" />
    <ProjectReference Include="..\Lychee.TypeFinder\Lychee.TypeFinder.csproj" />
